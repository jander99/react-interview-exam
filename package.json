{
	"name": "react-interview-exam",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/cypress": "^6.0.0",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^10.0.4",
		"@testing-library/user-event": "^7.1.2",
		"axios": "^0.21.2",
		"eslint-plugin-cypress": "^2.10.3",
		"eslint-plugin-istanbul": "^0.1.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-scripts": "3.4.1"
	},
	"scripts": {
		"start": "react-scripts -r @cypress/instrument-cra start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jest-environment-jsdom-sixteen",
		"ctest": "react-scripts test  --env=jest-environment-jsdom-sixteen --coverage --watchAll=false",
		"eject": "react-scripts eject",
		"cypress:open": "cypress open"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"plugin:cypress/recommended",
			"istanbul"
		],
		"rules": {
			"istanbul/prefer-ignore-reason": "error"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@cypress/code-coverage": "^3.7.6",
		"@cypress/instrument-cra": "^1.1.1",
		"axios-mock-adapter": "^1.18.1",
		"babel-plugin-istanbul": "^6.0.0",
		"cypress": "4.7.0",
		"istanbul-lib-coverage": "^3.0.0",
		"jest-environment-jsdom-sixteen": "^1.0.3",
		"nyc": "^15.0.1"
	}
}
